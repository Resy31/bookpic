<div class="contents row">
  <p style="font-size:25px;color:#000;"><%= @nickname %>さんの投稿一覧</p>
  <ul class='book-lists'>
    <% @posts.each do |post| %>
    <li class='list'>
      <%= link_to post_path(post.id), method: :get do %>
        <%= image_tag post.image, class: "book-img"%>
      <% end %>
    <% end %>
    </li>
  </ul>
</div>